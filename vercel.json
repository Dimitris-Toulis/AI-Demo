{
	"github": {
		"silent": true
	},
	"rewrites": [
		{
			"source": "(.*)",
			"destination": "/index.html"
		}
	],
	"trailingSlash": false,
	"headers": [
		{
			"source": "(.*)",
			"headers": [
				{
					"key": "Origin-Agent-Cluster",
					"value": "?1"
				},
				{
					"key": "Cross-Origin-Opener-Policy",
					"value": "same-origin"
				},
				{
					"key": "Cross-Origin-Embedder-Policy",
					"value": "require-corp"
				},
				{
					"key": "X-Content-Type-Options",
					"value": "nosniff"
				},
				{
					"key": "X-Frame-Options",
					"value": "DENY"
				},
				{
					"key": "Referrer-Policy",
					"value": "no-referrer"
				},
				{
					"key": "Origin-Trial",
					"value": "Ao/EoYXWM1kutFNKvSwhdSPJeHlaxyavCFI0KyYCHqYkWP+rwtEd+CXv42q3YiuJcOZ/AkzALflVCth2yF56xwgAAABbeyJvcmlnaW4iOiJodHRwczovL2FpLWRlbW8udG91bGlzLm5ldDo0NDMiLCJmZWF0dXJlIjoiV2ViQXNzZW1ibHlTaW1kIiwiZXhwaXJ5IjoxNjIzODAxNTk5fQ=="
				},
				{
					"key": "Content-Security-Policy",
					"value": "default-src 'self'; connect-src 'self' https://storage.googleapis.com https://tfhub.dev; script-src 'self' 'unsafe-eval' blob:; object-src 'none'; frame-ancestors 'none'; form-action 'none'; block-all-mixed-content; base-uri 'none'; style-src 'self'"
				}
			]
		},
		{
			"source": "/assets/(.*)",
			"headers": [
				{
					"key": "Cache-Control",
					"value": "public, max-age=604800, immutable"
				}
			]
		},
		{
			"source": "/favicon.ico",
			"headers": [
				{
					"key": "Cache-Control",
					"value": "public, max-age=360, stale-while-revalidate=86400"
				}
			]
		},
		{
			"source": "/manifest.webmanifest",
			"headers": [
				{
					"key": "Cache-Control",
					"value": "public, max-age=360, stale-while-revalidate=3600"
				}
			]
		},
		{
			"source": "/robots.txt",
			"headers": [
				{
					"key": "Cache-Control",
					"value": "public, max-age=86400"
				}
			]
		}
	]
}
